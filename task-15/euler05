a=int(input())

for i in range(a):
    b=int(input())
    c=[]
    d=[]
    f=0
    
    for i in range(1,b+1):
        c.append(i)
    
    for i in range(b+1,10000):
        d.append(i)
    

    for i in d:
        for j in c:
            if f!=b:
                if i%j==0:
                    f+=1
                    if f==b:
                        print(i)
                        break
                else:
                    f=0
            else:
                break
    
