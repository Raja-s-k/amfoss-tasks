import sys

a=int(input().strip())

if a >= 1 and a<=100000:
    
    for i in range(a):
        b=int(input().strip())
        
        if b >= 1 and b<=100000000:
            c=[]
            
            for i in range(1,b):
                if i%3==0 or i%5==0:
                    c.append(i)
                else:
                    pass
            
            print(sum(c))
                
                